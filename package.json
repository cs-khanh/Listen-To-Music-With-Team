{
  "name": "webmusic",
  "version": "1.0.0",
  "description": "Web music player with room-based synchronization",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "install-all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "socket.io": "^4.6.1",
    "youtube-sr": "^4.3.4"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "music",
    "youtube",
    "room",
    "synchronization"
  ],
  "author": "",
  "license": "MIT"
}
